  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run extract-critical && next build",
    "start": "node scripts/start-server.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "husky",
    "test": "echo \"No tests yet\" && exit 0",
    "analyze": "cross-env ANALYZE=true npm run build",
    "extract-critical": "node -r ts-node/register scripts/generate-style-assets.ts",
    "lighthouse": "lhci autorun",
    "lighthouse:local": "lighthouse http://localhost:3000 --view --output html --output-path ./lighthouse-report.html",
    "migrate:tokens": "ts-node scripts/migrate-tokens-example.ts",
    "migrate:scan": "ts-node scripts/migrate-tokens-example.ts --scan",
    "migration:scan:json": "ts-node scripts/migration-dashboard.ts --scan --json",
    "migrate:dry-run": "ts-node scripts/migrate-tokens-example.ts --dry-run",
    "migration:dashboard": "ts-node scripts/migration-dashboard.ts",
    "migration:scan": "ts-node scripts/migration-dashboard.ts --scan",
    "test:visual": "playwright test tests/visual-regression",
    "test:visual:update": "playwright test tests/visual-regression --update-snapshots"
  },
